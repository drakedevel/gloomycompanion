{
  "env": {
    "browser": true
  },
  "extends": "airbnb-base",
  "rules": {
    "block-scoped-var": 0,
    "camelcase": 0,
    "consistent-return": 0,
    "eqeqeq": 0,
    "func-names": 0,
    "guard-for-in": 0,
    "import/prefer-default-export": 0,
    "max-len": 0,
    "no-cond-assign": 0,
    "no-console": 0,
    "no-param-reassign": 0,
    "no-plusplus": 0,
    "no-redeclare": 0,
    "no-restricted-syntax": 0,
    "no-shadow": 0,
    "no-use-before-define": 0,
    "no-var": 0,
    "prefer-destructuring": 0,
    "radix": 0,
    "vars-on-top": 0
  }
}
