{
  "env": {
    "browser": true
  },
  "extends": "airbnb",
  "rules": {
    "camelcase": 0,
    "eqeqeq": 0,
    "func-names": 0,
    "guard-for-in": 0,
    "import/prefer-default-export": 0,
    "max-len": 0,
    "no-console": 0,
    "no-param-reassign": 0,
    "no-restricted-syntax": 0,
    "no-shadow": 0,
    "no-use-before-define": ["error", "nofunc"],
    "no-cond-assign": ["error", "except-parens"],
    "prefer-destructuring": 0,
    "radix": 0
  }
}
